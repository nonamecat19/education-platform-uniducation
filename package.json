{
  "name": "education-platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.dev next dev",
    "prod": "env-cmd -f .env.prod next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "env-cmd -f .env.dev drizzle-kit generate",
    "db:migrate": "env-cmd -f .env.dev tsx lib/db/migrate.ts",
    "db:migrate:prod": "env-cmd -f .env.prod tsx lib/db/migrate.ts",
    "db:drop": "env-cmd -f .env.dev drizzle-kit drop",
    "db:pull": "env-cmd -f .env.dev drizzle-kit introspect:pg",
    "db:studio": "env-cmd -f .env.dev drizzle-kit studio --config=drizzle.config.ts",
    "db:check": "env-cmd -f .env.dev drizzle-kit check:pg",
    "db:sync": "pnpm run db:generate && pnpm run db:migrate && pnpm run dev",
    "db:seed": "env-cmd -f .env.dev tsx lib/db/seed.ts",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@auth/drizzle-adapter": "^1.1.0",
    "@faker-js/faker": "^8.4.1",
    "@hookform/resolvers": "^3.4.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@sentry/nextjs": "^7.116.0",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.37.1",
    "@trpc/next": "^10.37.1",
    "@trpc/react-query": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18",
    "@vercel/kv": "^1.0.1",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "drizzle-dbml-generator": "^0.6.1",
    "drizzle-kit": "^0.21.2",
    "drizzle-orm": "^0.30.10",
    "drizzle-zod": "^0.5.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.378.0",
    "nanoid": "^5.0.7",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "pg": "^8.11.5",
    "postcss": "^8",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.4",
    "server-only": "^0.0.1",
    "sonner": "^1.4.41",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.9.4",
    "typescript": "^5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  }
}
